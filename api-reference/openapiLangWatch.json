              "type": "string"
            }
          },
          "error": {
            "type": "string",
            "nullable": true
          },
          "evaluations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Evaluation"
            }
          },
          "contexts": {
            "type": "array",
            "items": {}
          }
        }
      },
      "Timestamps": {
        "type": "object",
        "properties": {
          "inserted_at": {
